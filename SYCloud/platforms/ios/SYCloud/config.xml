<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sycloud.SYCloud" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <name>SYCloud</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="main.html?pg=home" />
    <access origin="*" />
    <!--添加这个就可以打开URL的HTML，即加载远程的URL网址-->
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <!--在Cordova中加载同一个域的URL是在App打开，跳转到其他却是用Safari打开，屏蔽一下两句，即会一直在app中进行跳转
     <allow-intent href="http://*/*" />
     <allow-intent href="https://*/*" />
     -->
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="local" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <preference name="CameraUsesGeolocation" value="false" />
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <feature name="Vibration">
        <param name="ios-package" value="CDVVibration" />
    </feature>
    <feature name="Toast">
        <param name="ios-package" value="Toast" />
    </feature>
    <feature name="VersionPlugin">
        <param name="ios-package" value="SYCVersionPlugin" />
    </feature>
    <feature name="CachePlugin">
        <param name="ios-package" value="SYCCachePlugin" />
    </feature>
    <feature name="SecurePlugin">
        <param name="ios-package" value="SYCSecurePlugin" />
    </feature>
    <feature name="ScanPlugin">
        <param name="ios-package" value="SYCScanPlugin" />
    </feature>
    <feature name="PaymentPlugin">
        <param name="ios-package" value="SYCPaymentPlugin" />
    </feature>
    <feature name="LoadingPlugin">
        <param name="ios-package" value="SYCLoadingPlugin" />
    </feature>
    <feature name="LocationPlugin">
        <param name="ios-package" value="SYCLocationPlugin" />
    </feature>
    <feature name="IntentPlugin">
        <param name="ios-package" value="SYCIntentPlugin" />
    </feature>
    <feature name="EventPlugin">
        <param name="ios-package" value="SYCEventPlugin" />
    </feature>
    <feature name="ResultPlugin">
        <param name="ios-package" value="SYCResultPlugin" />
    </feature>
    <feature name="PaymentCloudPlugin">
        <param name="ios-package" value="SYCPaymentCloudPlugin" />
    </feature>
    <feature name="RequestPlugin">
        <param name="ios-package" value="SYCRequestPlugin" />
    </feature>
</widget>
